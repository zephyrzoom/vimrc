/*
 * @author 707<707472783@qq.com>
 * This program is rewrite the 1.9~1.11 with for statement.
 */

#include <iostream>
int main()
{
    /* 1.9 */
    int sum = 0;
    for (int val = 50; val <= 100; val++)
    {
        sum += val;
    }
    std::cout << "sum from 50 to 100 is " << sum << std::endl;

    /* 1.10 */
}
